{
  "name": "orchestrator",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/orchestratora/orchestrator.git"
  },
  "scripts": {
    "ct": "git-cz",
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "node tools/scripts/build-all.js",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "clean": "run-s clean:*",
    "clean:dist": "rimraf dist",
    "clean:libs": "rimraf libs/*/dist",
    "pack": "run-s pack:*",
    "pack:files": "node tools/scripts/multi-copy.js LICENSE CODE_OF_CONDUCT.md dist/libs/*",
    "pack:libs": "npm run run:in:lib -- node ../../tools/scripts/pack-lib.js",
    "release": "lerna publish --yes --conventional-graduate",
    "release:tag": "lerna publish --yes --conventional-prerelease --dist-tag $RELEASE_TAG --preid $RELEASE_TAG",
    "postversion": "npm run run:in:lib -- node ../../tools/scripts/version-lib.js",
    "run:in:lib": "lerna exec --concurrency 1 --stream --"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.4",
    "@angular/cdk": "^9.2.2",
    "@angular/common": "^9.1.4",
    "@angular/compiler": "^9.1.4",
    "@angular/core": "^9.1.4",
    "@angular/forms": "^9.1.4",
    "@angular/platform-browser": "^9.1.4",
    "@angular/platform-browser-dynamic": "^9.1.4",
    "@angular/router": "^9.1.4",
    "@orchestrator/gen-io-ts": "^3.0.1",
    "core-js": "^3.6.4",
    "fp-ts": "^2.5.4",
    "io-ts": "^2.2.1",
    "ng-dynamic-component": "^6.1.1-no-barrels.1",
    "rxjs": "^6.5.5",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.4",
    "@angular-devkit/build-ng-packagr": "^0.901.4",
    "@angular/cli": "^9.1.4",
    "@angular/compiler-cli": "^9.1.4",
    "@angular/language-service": "^9.1.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nrwl/angular": "^9.2.3",
    "@nrwl/jest": "^9.2.3",
    "@nrwl/workspace": "^9.2.3",
    "@types/copyfiles": "^2.1.1",
    "@types/jest": "25.1.4",
    "@types/node": "^13.7.7",
    "codelyzer": "^5.2.1",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "fast-glob": "^3.2.2",
    "husky": "^4.2.3",
    "jasmine-marbles": "^0.6.0",
    "jest": "^25.2.4",
    "jest-preset-angular": "^8.1.3",
    "lerna": "^3.20.2",
    "lint-staged": "^10.0.8",
    "ng-packagr": "^9.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tsickle": "^0.38.1",
    "tslib": "^1.11.1",
    "tslint": "^6.1.2",
    "typescript": "~3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm-run-all -p affected:lint affected:test"
    }
  },
  "lint-staged": {
    "**/*.{ts,js,json,html,?css}": [
      "npm run format -- --files",
      "git add"
    ]
  }
}
